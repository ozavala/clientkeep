  phone : string
  website : string
  address : string
  city : string
  state : string
  country : string
  zip : string
  email : string
  password : string
  role_id : int <<FK>>
  status : Enum '(Active,Inactive,Deleted)'
  created_at : datetime
  updated_at : datetime
}

