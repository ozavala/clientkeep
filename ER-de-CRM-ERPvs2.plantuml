  iva_paid : decimal        ' IVA soportado
  iva_collected : decimal   ' IVA repercutido
  iva_balance : decimal     ' Saldo a pagar o devolver
  created_at : datetime     ' Fecha de generaciÃ³n
  updated_at : datetime
}

entity(JournalEntries) {
  +id : int <<PK>>
  tenant_id : int <<FK>>
  period_start : 
  date : date
  description : string
  reference : string
  created_at : datetime
  updated_at : datetime
}

entity(JournalLines) {
  +id : int <<PK>>
  tenant_id : int <<FK>>
